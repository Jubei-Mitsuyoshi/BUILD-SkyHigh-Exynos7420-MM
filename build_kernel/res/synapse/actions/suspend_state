if [ -e /su/xbin/busybox ]; then
	BB=/su/xbin/busybox;
elif [ -e /system/xbin/busybox ]; then
	BB=/system/xbin/busybox;
fi;

if [ -f "$1" ]; then
	if [[ ! -z "$2" ]]; then
		PSM=$($BB cat /sys/kernel/power_suspend/power_suspend_mode);
		
		if [ "$PSM" == "1" ]; then
			$BB echo "$2" > "$1"
		fi;
	fi;

	$BB echo "$($BB cat "$1")";
else
	$BB echo "0";
fi;
