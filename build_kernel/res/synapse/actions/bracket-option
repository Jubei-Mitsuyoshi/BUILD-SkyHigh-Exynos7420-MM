if [ -e /su/xbin/busybox ]; then
	BB=/su/xbin/busybox;
elif [ -e /system/xbin/busybox ]; then
	BB=/system/xbin/busybox;
elif [ -e /system/bin/busybox ]; then
	BB=/system/bin/busybox;
fi;

if [ -f "$1" ]; then
	if [[ ! -z $2 ]]; then
		$BB echo "$2" > "$1" 2> /dev/null;
	fi;

	$BB echo "$($BB cat "$1" | $BB sed -n -e '/\[[^]]/s/^[^[]*\[\([^]]*\)].*$/\1/p')";
fi;
